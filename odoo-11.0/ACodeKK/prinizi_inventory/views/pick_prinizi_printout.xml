                                    <!--<td style="border-color:black;padding:0px 5px 0px 5px !important;vertical-align:middle; text-align:left"><t t-esc="line.get('size', '')"/></td>-->
                                    <!--<td style="border-color:black;padding:0px 5px 0px 5px !important;vertical-align:middle"><t t-esc="line.get('product_uom_qty', '')"/></td>-->
                                    <td style="border-color:black;padding:0px 5px 0px 5px !important;vertical-align:middle"><t t-esc="line.get('print_qty', '')"/></td>
