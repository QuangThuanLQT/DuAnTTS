        <template id="prinizi_inventory.printout_picking_prinizi_pick">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="prinizi_inventory.picking_prinizi_pick_printout_template" t-lang="doc.partner_id.lang"/>
                </t>
            </t>
        </template>

        <record id="picking_prinizi_pick_printout_format" model="report.paperformat">
            <field name="name">phiếu lấy hàng in</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">13</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">3</field>
            <field name="margin_right">3</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">9</field>
            <field name="dpi">90</field>
        </record>

        <report
                id="picking_prinizi_pick_printout"
                string="Phiếu lấy hàng in"
                model="stock.picking"
                report_type="qweb-pdf"
                file="prinizi_inventory.printout_picking_prinizi_pick"
                name="prinizi_inventory.printout_picking_prinizi_pick"
        />

        <record id="prinizi_inventory.picking_prinizi_pick_printout" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="picking_prinizi_pick_printout_format"/>
        </record>




