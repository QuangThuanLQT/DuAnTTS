<?xml version="1.0" encoding="UTF-8"?>
<templates id="template_salary_sale" xml:space="preserve">
    <t t-name="salary_sale_export_button">
        <button type="button" class='btn btn-sm btn-default o_buttons_print_salary_sale'>Xuất thông tin chi tiết</button>
    </t>

    <t t-name="SearchSalarySale">
        <div class="sky-search btn-group btn-group-sm">
            <div style="margin-left: 0px; margin-top: 2px; padding-right: 10px;">
                <span style="float: left;">
                    <select class="sky_select_salary_sale form-control" style="height: 30px;" >
                        <option t-foreach="sky_fields" t-as="field" t-att-value="field[0]">
                            <t t-esc="field[1]"/>
                        </option>
                    </select>
                </span>
                <span style="float: left; margin-left: 2px;">
                    <select class="salary_sale form-control" style="height: 30px;" >
                        <option value="01">1</option>
                        <option value="02">2</option>
                        <option value="03">3</option>
                        <option value="04">4</option>
                        <option value="05">5</option>
                        <option value="06">6</option>
                        <option value="07">7</option>
                        <option value="08">8</option>
                        <option value="09">9</option>
                        <option value="10">10</option>
                        <option value="11">11</option>
                        <option value="12">12</option>
                    </select>
                </span>
            </div>
        </div>
    </t>

</templates>