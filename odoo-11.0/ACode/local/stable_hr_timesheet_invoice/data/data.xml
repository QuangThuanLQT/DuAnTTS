            <!--<field name="use_tasks">0</field>-->
            <!--<field name="allow_timesheets">1</field>-->
        <!--</record>-->

        <record id="timesheet_journal" model="account.journal">
            <field name="name">Timesheet Journal</field>
            <field name="type">general</field>
            <field name="code">TS</field>
            <!--<field name="default_debit_account_id">TIMESHEET</field>-->
            <!--<field name="default_credit_account_id">TIMESHEET</field>-->
        </record>

        <record id="sale_journal" model="account.journal">
