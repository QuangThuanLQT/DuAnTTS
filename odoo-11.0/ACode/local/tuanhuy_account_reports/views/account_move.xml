        <record id="view_filter_report_111_112" model="ir.ui.view">
            <field name="name">account.move.line.filter.report</field>
            <field name="model">account.move.line</field>
            <field name="arch" type="xml">
                <search string="Search Journal Items">
                    <field name="name" filter_domain="['|', ('name','ilike',self), ('ref','ilike',self)]" string="Tham chiếu"/>
                    <field name="ref" string="Số chứng từ" filter_domain="[('ref','=',self)]"/>
                    <field name="name_sub" string="Diễn giải" filter_domain="[('name','ilike',self)]"/>
                    <!--<field name="product_id" string="Sản phẩm"/>-->
                    <field name="date"/>
                    <field name="tax_line_id"/>
                    <field name="tax_ids" />
                    <separator/>
                    <filter string="Chưa vào sổ" domain="[('move_id.state','=','draft')]" help="Unposted Journal Items"/>
                    <filter name="posted" string="Đã vào sổ" domain="[('move_id.state','=','posted')]" help="Posted Journal Items"/>
                    <separator/>
                    <filter string="Chưa được đối soát" domain="[('full_reconcile_id', '=', False), ('balance','!=', 0), ('account_id.reconcile','=',True)]" help="Journal items where matching number isn't set" name="unreconciled"/>
                    <separator/>
                    <filter string="Phải trả" domain="[('account_id.internal_type', '=', 'payable')]" help="From Payable accounts" name="payable"/>
                    <filter string="Phải thu" domain="[('account_id.internal_type', '=', 'receivable')]" help="From Receivable accounts" name="receivable"/>
                    <separator/>
                    <field name="move_id" string="Number (Move)"/>
                    <field name="account_id"/>
                    <field name="partner_id"/>
                    <field name="journal_id"/>
                    <field name="account_doiung"/>
                </search>
            </field>
        </record>

