<odoo>
    <data>

        <record id="view_move_line_form_inherit" model="ir.ui.view">
            <field name="name">account.move.line.form</field>
            <field name="model">account.move.line</field>
            <field name="inherit_id" ref="account.view_move_line_form"/>
            <field name="arch" type="xml">
                <field name="ref" position="attributes">
                    <attribute name="attrs">{'invisible':['|','|','|','|',('ref_purchase_order','!=',False),('ref_account_voucher','!=',False)
                        ,('ref_account_payment_unc','!=',False),('ref_sale_order','!=',False),('ref_account_payment_gbn','!=',False)]}</attribute>
                </field>
                <field name="ref" position="after">
                    <field name="ref_purchase_order" attrs="{'invisible':[('ref_purchase_order','=',False)]}"/>
                    <field name="ref_account_voucher" attrs="{'invisible':[('ref_account_voucher','=',False)]}"/>
                    <field name="ref_account_payment_unc" attrs="{'invisible':[('ref_account_payment_unc','=',False)]}"/>
                    <field name="ref_sale_order" attrs="{'invisible':[('ref_sale_order','=',False)]}"/>
                    <field name="ref_account_payment_gbn" attrs="{'invisible':[('ref_account_payment_gbn','=',False)]}"/>
                </field>
            </field>
        </record>

        <template id="assets_backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/tuanhuy_account_reports/static/src/js/search_by_range.js"></script>
            </xpath>
        </template>
        <menuitem id="account_detail_report_menu" name="Báo Cáo Chi Tiết Tài Khoản"
                  parent="account.menu_finance_reports" sequence="5"/>

        <record id="account_detail_report_view" model="ir.ui.view">
            <field name="name">Báo Cáo Chi Tiết Tài Khoản</field>
            <field name="model">account.move.detail</field>
            <field name="arch" type="xml">
                <form>
                    <label string=""/>
                    <group>
                        <group>
                            <field name="account_id" options="{'no_create': True, 'no_open': True}"/>
                            <field name="start_date" string="Từ Ngày"/>
                            <field name="end_date" string="Đến Ngày"/>
                            <field name="partner_id"/>
                        </group>
                        <group>
                            <group>
                                <field name="no_before"/>
                            </group>
                            <group>
                                <field name="co_before"/>
                            </group>
                        </group>
                        <button name="return_acction_report" string="Xem" type="object" default_focus="1"
                                class="oe_highlight" width="20px"/>
                        <br/>
                        <field name="account_move_line_ids" nolabel="1">
                            <tree options="{'no_create': True}">
                                <field name="date"/>
                                <field name="ref"/>
                                <field name="name"/>
                                <field name="partner_id"/>
                                <field name="account_id" options="{'no_open':True}"
                                       domain="[('company_id', '=', company_id)]"/>
                                <field name="account_doiung"/>
                                <field name="debit" sum="Tổng Nợ"/>
                                <field name="credit" sum="Tổng số báo có"/>
                                <field name="debit_amount_residual" sum="Total debit amount_residual"
                                       string="Nợ còn lại"/>
                                <field name="credit_amount_residual" sum="Total credit amount_residual"
                                       string="Có còn lại"/>
                                <field name="amount_residual" sum="Total amount_residual" on_change="1"
                                       modifiers="{'readonly': true}"/>
                            </tree>
                        </field>

                        <!--<group>-->
                        <!--<field name="start_date" string="Start Date"/>-->
                        <!--<field name="end_date" string="End Date"/>-->
                        <!--</group>-->
                    </group>
                    <footer>
                        <!--<button name="print_report" string="Print" type="object" default_focus="1"-->
                        <!--class="oe_highlight"/>-->
                        <!--or-->
                        <button string="Huỷ" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


