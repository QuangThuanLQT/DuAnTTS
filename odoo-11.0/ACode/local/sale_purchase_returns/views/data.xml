<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="seq_sale_order_return" model="ir.sequence">
            <field name="name">Sales Order Return</field>
            <field name="code">sale.order.return</field>
            <field name="prefix">RT</field>
            <field name="suffix">/%(month)s%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_purchase_order_return" model="ir.sequence">
            <field name="name">Purchase Order Return</field>
            <field name="code">purchase.order.return</field>
            <field name="prefix">RTP</field>
            <field name="suffix">/%(month)s%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>